<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
          jcr:primaryType="cq:Page">
    <jcr:content
            cq:template="/conf/wbank/settings/wcm/templates/content-page-template"
            jcr:primaryType="cq:PageContent"
            jcr:title="Page not found"
            sling:resourceType="kbank/components/page"
            pageTitle="404">
        <root
                jcr:primaryType="nt:unstructured"
                sling:resourceType="kbank/components/container">
            <container
                    cq:styleIds="[1554340406437]"
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="kbank/components/container"
                    layout="responsiveGrid">
                <container
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="kbank/components/container">
                    <cq:responsive jcr:primaryType="nt:unstructured">
                        <default
                                jcr:primaryType="nt:unstructured"
                                offset="0"
                                width="6"/>
                        <phone
                                jcr:primaryType="nt:unstructured"
                                offset="0"
                                width="12"/>
                    </cq:responsive>
                    <separator
                            cq:styleIds="[1571075919520]"
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="kbank/components/separator">
                        <cq:responsive jcr:primaryType="nt:unstructured">
                            <phone
                                    jcr:primaryType="nt:unstructured"
                                    behavior="hide"/>
                        </cq:responsive>
                    </separator>
                    <title
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Ruh-Roh! Page Not Found"
                            sling:resourceType="kbank/components/title"
                            type="h1">
                        <cq:responsive jcr:primaryType="nt:unstructured">
                            <default
                                    jcr:primaryType="nt:unstructured"
                                    offset="0"
                                    width="12"/>
                            <phone
                                    jcr:primaryType="nt:unstructured"
                                    offset="0"
                                    width="12"/>
                        </cq:responsive>
                    </title>
                    <text
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="kbank/components/text"
                            text="&lt;p>We're sorry, we are unable to fetch this page!&lt;/p>&#xd;&#xa;"
                            textIsRich="true">
                        <cq:responsive jcr:primaryType="nt:unstructured">
                            <phone
                                    jcr:primaryType="nt:unstructured"
                                    offset="0"
                                    width="12"/>
                        </cq:responsive>
                    </text>
                </container>
                <image
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="kbank/components/image"
                        fileReference="/content/dam/wbank/en/site/not-found.jpg">
                    <cq:responsive jcr:primaryType="nt:unstructured">
                        <default
                                jcr:primaryType="nt:unstructured"
                                offset="0"
                                width="6"/>
                        <phone
                                jcr:primaryType="nt:unstructured"
                                offset="0"
                                width="12"/>
                    </cq:responsive>
                </image>
            </container>
        </root>
    </jcr:content>
</jcr:root>

<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
          jcr:primaryType="cq:Page">
    <jcr:content
            cq:template="/conf/kbank/settings/wcm/templates/adventure-page-template"
            jcr:primaryType="cq:PageContent"
            sling:resourceType="kbank/components/page"
            socialMedia="[facebook,pinterest]">
        <root
                jcr:primaryType="nt:unstructured"
                sling:resourceType="kbank/components/container"
                layout="responsiveGrid">
            <container
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="kbank/components/container"
                    layout="responsiveGrid">
                <breadcrumb
                        cq:styleIds="[1570236596118]"
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="kbank/components/breadcrumb"
                        disableShadowing="false"
                        hideCurrent="false"
                        showHidden="false"
                        startLevel="4"/>
                <carousel
                        cq:styleIds="[1570236158571]"
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="kbank/components/carousel">
                    <image
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="kbank/components/image"/>
                </carousel>
                <container_fixed
                        cq:styleIds="[1554340406437]"
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="kbank/components/container"
                        layout="responsiveGrid">
                    <title
                            cq:styleIds="[1568996484405]"
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="kbank/components/title"/>
                    <container
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="kbank/components/container"
                            layout="responsiveGrid">
                        <cq:responsive jcr:primaryType="nt:unstructured">
                            <default
                                    jcr:primaryType="nt:unstructured"
                                    offset="0"
                                    width="3"/>
                            <tablet
                                    jcr:primaryType="nt:unstructured"
                                    offset="0"
                                    width="12"/>
                            <phone
                                    jcr:primaryType="nt:unstructured"
                                    offset="0"
                                    width="12"/>
                        </cq:responsive>
                        <contentfragment
                                cq:styleIds="[1571166694176]"
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="kbank/components/contentfragment"
                                displayMode="multi"
                                elementNames="[activity,adventureType,tripLength,groupSize,difficulty,price]"
                                fragmentPath="/content/dam/kbank-shared/en/adventures/surf-camp-in-costa-rica/surf-camp-costa-rica"
                                text="Surf Camp Costa Rica Activity Surfing Adventure Type Overnight Trip Trip Length 5 Days Group Size 3-6 Difficulty Beginner Price $3400 USD"
                                variationName="master">
                            <cq:responsive jcr:primaryType="nt:unstructured">
                                <phone
                                        jcr:primaryType="nt:unstructured"
                                        offset="0"
                                        width="7"/>
                                <tablet
                                        jcr:primaryType="nt:unstructured"
                                        offset="0"
                                        width="9"/>
                            </cq:responsive>
                        </contentfragment>
                        <title
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Share this Adventure"
                                sling:resourceType="kbank/components/title"
                                type="h5">
                            <cq:responsive jcr:primaryType="nt:unstructured">
                                <tablet
                                        jcr:primaryType="nt:unstructured"
                                        offset="0"
                                        width="3"/>
                                <phone
                                        jcr:primaryType="nt:unstructured"
                                        offset="0"
                                        width="5"/>
                            </cq:responsive>
                        </title>
                        <sharing
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="kbank/components/sharing">
                            <cq:responsive jcr:primaryType="nt:unstructured">
                                <tablet
                                        jcr:primaryType="nt:unstructured"
                                        offset="0"
                                        width="3"/>
                                <phone
                                        jcr:primaryType="nt:unstructured"
                                        offset="0"
                                        width="5"/>
                            </cq:responsive>
                        </sharing>
                    </container>
                    <tabs
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="kbank/components/tabs"
                            accessibilityLabel="adventure trip details">
                        <cq:responsive jcr:primaryType="nt:unstructured">
                            <default
                                    jcr:primaryType="nt:unstructured"
                                    offset="0"
                                    width="9"/>
                            <tablet
                                    jcr:primaryType="nt:unstructured"
                                    offset="0"
                                    width="12"/>
                            <phone
                                    jcr:primaryType="nt:unstructured"
                                    offset="0"
                                    width="12"/>
                        </cq:responsive>
                        <item_1594238312974
                                cq:panelTitle="Overview"
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="kbank/components/contentfragment"
                                text="\0"/>
                        <item_1570890140330
                                cq:panelTitle="Itinerary"
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="kbank/components/contentfragment"/>
                        <item_1570890147607
                                cq:panelTitle="What to Bring"
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="kbank/components/contentfragment"/>
